# HealthLink MVP Environment Variables
# 复制此文件为 .env 并填入实际值

# 数据库配置 (MySQL切换时需要)
MYSQL_USER=healthlink_user
MYSQL_PASSWORD=your_mysql_password

# AI模型API配置
NLP_API_ENDPOINT=https://api.example.com/nlp/v1
NLP_API_KEY=your_nlp_api_key
ASR_API_ENDPOINT=https://api.example.com/asr/v1
ASR_API_KEY=your_asr_api_key

# 安全配置
JWT_SECRET_KEY=your_jwt_secret_key_here_make_it_long_and_random
ENCRYPTION_KEY=your_32_byte_encryption_key_here

# Redis配置 (后续切换时需要)
REDIS_PASSWORD=your_redis_password

# 开发环境配置
DEBUG=true
LOG_LEVEL=INFO

# 外部服务配置 (可选)
PROMETHEUS_ENABLED=true
HEALTH_CHECK_ENABLED=true